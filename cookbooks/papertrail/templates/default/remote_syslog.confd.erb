<% @name ||= 'remote_syslog' %>
NAME="<%= @name %>"
DAEMON="/usr/sbin/${NAME}"
CONFIG="<%= @config %>"
PIDFILE="/var/run/${NAME}.pid"
DAEMON_ARGS="-c ${CONFIG} --pid-file ${PIDFILE}"
