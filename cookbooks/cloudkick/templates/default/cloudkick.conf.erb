#
# Cloudkick Congfiguration
#
# See the following URL for the most up to date documentation:
#   https://support.cloudkick.com/Agent/Cloudkick.conf
#
# The keys in cloudkick.conf are tied to your entire account,
# so you can deploy the same file across all of your machines.
#
# oAuth consumer key
oauth_key <%= @oauth_key %>
# oAuth consumer secret
oauth_secret <%= @oauth_secret %>
<% if @tags %>
# tags for this node
tags <%= @tags.join(", ") %>
<% end %>